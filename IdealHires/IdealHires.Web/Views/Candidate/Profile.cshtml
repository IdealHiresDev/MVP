                        <div class="f1">
                            <div class="f1-steps text-center">
                                <div class="f1-progress">
                                    <div class="f1-progress-line" data-now-value="16.66" data-number-of-steps="6" style="width: 6%;"></div>
                                </div>
                                <div id="generalDiv" class="f1-step active" href="#general">
                                    <div class="f1-step-icon"><i class="fa fa-user"></i></div>
                                    <p class="font-weight-bold">General</p>
                                </div>
                                <div id="contactDiv" class="f1-step" href="#contact">
                                    <div class="f1-step-icon"><i class="fa fa-key"></i></div>
                                    <p>Contact</p>
                                </div>
                                <div id="preferencesDiv" class="f1-step" href="#preferences">
                                    <div class="f1-step-icon"><i class="fa fa-user"></i></div>
                                    <p>Preferences</p>
                                </div>
                                <div id="workexpDiv" class="f1-step" href="#workexp">
                                    <div class="f1-step-icon"><i class="fa fa-user"></i></div>
                                    <p>Work Exp</p>
                                </div>
                                <div id="educationDiv" class="f1-step" href="#education">
                                    <div class="f1-step-icon"><i class="fa fa-user"></i></div>
                                    <p>Education</p>
                                </div>
                                <div id="previewDiv" class="f1-step" href="#preview">
                                    <div class="f1-step-icon"><i class="fa fa-user"></i></div>
                                    <p>Preview</p>
                                </div>
                            </div>
                            <div class="wizard-form container-fluid">
                                <fieldset class="mb-4 mt-3" id="general">
                                    @{Html.RenderAction("General"); }
                                </fieldset>
                                <fieldset class="mb-4 mt-3" id="contact">
                                    @{Html.RenderAction("Contact"); }
                                </fieldset>
                                <fieldset class="mb-4 mt-3" id="preferences">
