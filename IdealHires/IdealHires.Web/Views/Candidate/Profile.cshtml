@{
    ViewBag.Title = "Profile";
}
<section>
    <div class="mt-5 mb-3">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 text-left mb-4">
                    <h3 class="blue-clr"><i class="fa fa-user-alt pr-2" aria-hidden="true"></i> Create Candidate Profile</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-10 align-self-center">
